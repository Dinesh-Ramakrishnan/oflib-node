{
    "id" : "ofp_queue_stats",
    "type" : "object",
    "properties" : [
        "port_no" : {"$ref" : "../types/port_id_req.json"},
        "queue_no" : {"$ref" : "../types/queue_id_req.json"},

        "tx_bytes" : {       /* Number of transmitted bytes. */
            "extends" : {"$ref" : "../types/uint64.json},
            "description" : "if not present, counter is not supported"
        },
        "tx_packets" : {     /* Number of transmitted packets. */
            "extends" : {"$ref" : "../types/uint64.json},
            "description" : "if not present, counter is not supported"
        },
        "tx_errors" : {      /* Number of packets dropped due to overrun. */
            "extends" : {"$ref" : "../types/uint64.json},
            "description" : "if not present, counter is not supported"
        }
    ]
