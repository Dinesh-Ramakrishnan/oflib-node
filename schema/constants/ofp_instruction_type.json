{
    "id" : "ofp_instruction_type",
    "type" : "string",
    "enum" : [
        "OFPIT_GOTO_TABLE",       /* Setup the next table in the lookup
                                     pipeline */
        "OFPIT_WRITE_METADATA",   /* Setup the metadata field for use later in
                                     pipeline */
        "OFPIT_WRITE_ACTIONS",    /* Write the action(s) onto the datapath action
                                     set */
        "OFPIT_APPLY_ACTIONS",    /* Applies the action(s) immediately */
        "OFPIT_CLEAR_ACTIONS",    /* Clears all actions from the datapath
                                     action set */

        "OFPIT_EXPERIMENTER"      /* Experimenter instruction */
    ]
}
